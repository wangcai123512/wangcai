        function InitalDateInput() {
            $('.datepicker').datepicker(
            {
                language: "zh-CN",
                format: "yyyy/mm/dd",
                todayBtn: "linked",
                todayHighlight: true,
                autoclose: true,
                clearBtn: true,//清除按钮
                endDate: NowDate,
            });
        }

        function FileInputBaseSetting(selector) {
            $(selector).fileinput({
                showUpload: false,
                showRemove: false,
                showPreview: false,
                showCancel: false,
                showCaption: true,
                autoReplace: true,
                browseLabel: '',
                layoutTemplates: feilInputLayoutTemplates,
                maxFileCount: 1,
                mainClass: "input-group-lg",
                browseClass: "btn bg-purple2 btn-sm",
                initialPreview: [],
                showPreview: false,
                language: 'zh',
                maxFileSize: "5120",
                msgSizeTooLarge: "文件不可以超过5M，请重新上传。",
                initialCaption: "5M以内文件"
               
               
            });

            $(selector).on("fileuploaderror", function (event, data, msg) {
               //q alert(msg);
            });
        }

        function GetSupplier() {
            var supplier
            $.ajax({
                url: "/InternalAPI/GetPayeeJson",
                async: false,
                dataType: "json",
                success: function (d) {
                    supplier = d;
                }
            });
            return supplier;
        }
        function GetClient() {
            var client
            $.ajax({
                url:"/InternalAPI/GetCustomer",
                async: false,
                dataType: "json",
                success: function (d) {
                    for (var i = 0; i < d.length; i++) {
                        d[i]["id"] = d[i]["value"];
                        d[i]["value"] = d[i]["label"];
                    }
                    client = d;
                }
            });
            return client;
        }
        function NewGUID() {
            var GUID;
            $.ajax({
                url: "/ReceivablesRecord/NewGuid",
                async: false,
                dataType: "text",
                success: function (d) {
                    GUID = d.toString();
                }
            });
            return GUID;
        }

        function conveterParamsToJson(paramsAndValues) {
            var jsonObj = {};

            var param = paramsAndValues.split("&");
            for (var i = 0; param != null && i < param.length; i++) {
                var para = param[i].split("=");
                jsonObj[para[0]] = para[1];
            }

            return jsonObj;
        }
        function changeLanguage() {
            var language;
            language = $('#Languages').val();
            $.ajax({
                url: "/InternalAPI/ChangeLanguage",
                async: false,
                cache: false,
                data: "language=" + language,
                dataType: "json",
                success: function (d) {
                    if (d.success == true) {
                        $('#Languages').val(language);
                        $('#Languagess').val(language);
                        alert("修改成功");
                    } else {
                        $('#Language').val('@Session["Language"]');
                    }
                },
                error: function () {
                    $('#Language').val('@Session["Language"]');
                }
            });
        }

        function Undone(){
            alert("该功能尚未开通，敬请期待");
        }
