    var str
    var amt = 0
    var SumAmount = 0
    function Preview() {
        document.getElementById('Settlement').value = '1';
        QuarterDate = $("#quarters").val();
        var strs = new Array(); //定义一数组
        strs = QuarterDate.split("/");
        year = strs[0];
        quarter = strs[1];
        str = "";
        amt = 0;
        SumAmount = 0;
        if (!DateCheck()) {
