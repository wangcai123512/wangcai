    //产生GUID
    function NewGUID() {
        var GUID;
        $.ajax({
            url: "/PaymentDeclareCostSpending/NewGuid",
            async: false,
            dataType: "text",
            success: function (d) {
                GUID = d.toString();
            }
        });
        return GUID;
    }

    function removeClass() {
