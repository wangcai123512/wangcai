        { field: 'SumAmount', align: 'center', title: '可抵扣金额(本币)', editable: { type: 'text', title: '可抵扣金额' }, formatter: DecimalFmter },
        { field: 'Currency', align: 'center', title: '货币', formatter: Handle }
